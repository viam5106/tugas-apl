#include <conio.h>  
#include <iostream>  
#include <math.h>  
using namespace std;  
  
int main() {  
    int A,b,hasil;
    
    cout << " Masukkan nilai A (Bilangan)= ";  
    cin >> A;  
    cout << endl << " Masukkan nilai b(pangkat) = ";  
    cin >> b;  
    hasil= pow(A, b);  
  
    cout << endl << " Hasil dari " << A << " pangkat " << b << " adalah " << hasil << endl;  
    getch();  
}  
